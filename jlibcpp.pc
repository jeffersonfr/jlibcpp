prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: @module@
Description: Portable C++ library.
Version: @version@
Requires: @requires@
URL: http://sourceforge.net/projects/jlibcpp
Libs: -L${libdir} @libs@ -ljlibcpp -lrt -ldl -lpthread -std=c++14
Cflags: @defines@ -fPIC -std=c++14 -D_GNU_SOURCE -D_REENTANT -D_FILE_OFFSET_BITS=64 -D_LARGEFILE_SOURCE -I${includedir}/jlibcpp
